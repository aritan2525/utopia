{
  "routes": [
    {
      "src": "/protected",
      "methods": ["GET"],
      "headers": [
        {
          "key": "WWW-Authenticate",
          "value": "Basic realm=\"Secure Area\""
        }
      ],
      "handle": "vercel/functions/basic-auth.js"
    }
  ],
  "functions": {
    "basic-auth.js": {
      "runtime": "vercel-node@2.4.0"
    }
  }
}
